{
    "env": {
        "name": "mixing",
        "obs_clip": false,
        "obs_norm": true,
        "obs_noise": 0.0
    },
    "naming": {
        "env": true,
        "agent": true,
        "tag": false,
        "time": false
    },
    "n_avg": 3,
    "n_stp_max": 100000,
    "trainer": {
        "style": "td",
        "mem_size": 100000,
        "btc_size": 64,
        "n_stp_unroll": 50,
        "render_every": 1000000
    },
    "agent": {
        "type": "dqn",
        "gamma": 0.99,
        "tgt_update": 50,
        "exploration": {
            "type": "linear",
            "start": 0.5,
            "end": 0.05,
            "n_decay": 2000000
        },
        "value": {
            "type": "q_value",
            "network": {
                "type": "fc",
                "trunk": {
                    "arch": [64,64],
                    "actv": "relu"
                },
                "heads": {
                    "nb": 1,
                    "arch": [[]],
                    "actv": ["relu"],
                    "final": ["linear"]
                }
            },
            "optimizer": {
                "type": "adam",
                "lr": 5.0e-4,
                "grd_clip": null
            },
            "loss": {
                "type": "mse_dqn"
            }
        },
        "termination": {
            "type": "bootstrap"
        }
    }
}
